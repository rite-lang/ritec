import std:result:Result

fn main(args: [str])
  let sources = span:new_sources()
  let sources, id = sources
    |> span:add_source(span:load_source("test/test.ri")?)

  let span = span:Span(4, 8, 0)

  let diagnostic = diagnostic:new(
    diagnostic:Error
    "E0001"
    "this is an error"
  ) |> diagnostic:add_label("this is a label", span)

  diagnostic:print(diagnostic, sources)

  Ok(void)
