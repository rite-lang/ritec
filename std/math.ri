//! Math utilities.

/// Compute the maximum of two numbers.
pub fn min(a, b)
  match a < b
  | true  -> a
  | false -> b

/// Compute the minimum of two numbers.
pub fn max(a, b)
  match a < b
  | true  -> b
  | false -> a

/// Add two numbers.
pub fn add(a, b)
  a + b

/// Subtract two numbers.
pub fn sub(a, b)
  a - b
