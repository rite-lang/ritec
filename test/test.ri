import std:list
import std:result

type A =
  | A(a: &int)

fn main()
  let f = add(_, 1)

  [1, 2, 3], [4, 5, 6]
  |> list:concat
  |> list:filter(is_even)
  |> list:try_map(not_five)

fn add(a, b)
  a + b

fn is_even(x)
  (x % 2) == 0

fn not_five(x)
  match x == 5
  | true -> result:Err("Five")
  | false -> result:Ok(x)
