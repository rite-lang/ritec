import std:io
import std:result:Result
import std:list
import std:debug
import std:dict
import std:dict:Dict

fn main()
  let x =
    [1, 2, 3, 4]
    |> list:chunk_pairs
    |> dict:from_pairs
    |> debug:print

  let dict = 
    dict:new()
    |> dict:insert("a", 1)
    |> dict:insert("b", 2)
    |> dict:pairs

  io:println(f"dict: {dict}")
